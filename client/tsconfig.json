{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* If NOT transpiling with TypeScript: */
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "noEmit": true,

    /* If your code runs in the DOM: */
    "lib": ["es2022", "dom", "dom.iterable"],

    "types": ["react", "react-dom"],
    "jsx": "preserve"
  },
  "include": [".", "../shared"]
}
